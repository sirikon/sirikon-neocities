    <link href="https://sirikon.neocities.org/posts/{{post.slug}}" rel="alternate">
    <!-- <published>2024-03-13T17:06:06+01:00</published>
    <updated>2024-03-13T17:06:06+01:00</updated> -->
    <author>
      <name>Carlos Fdez. Llamas</name>
      <email>hello@sirikon.me</email>
    </author>
    <content type="html">{{post.content_html_escaped}}</content>
  </entry>
  {% endfor %}

</feed>
{{content|safe}}
