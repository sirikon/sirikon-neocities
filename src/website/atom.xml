<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <id>https://sirikon.neocities.org/</id>
  <title>Sirikon's Neocities</title>
  <link href="https://sirikon.neocities.org/atom.xml" rel="self" />
